{
    "name": "shopack/yii2-base-frontend",
    "type": "yii2-extension",
    "authors": [
        {
            "name": "Kambiz Zandi",
            "email": "kambizzandi@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "shopack\\base\\frontend\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4.0",
        "yiisoft/yii2": "~2.0.45",
        "bizley/jwt": "^3.0",
        "kartik-v/yii2-widget-sidenav": "*",
        "kartik-v/yii2-tabs-x": "@dev",
        "bower-asset/persian-datepicker": "^1.1",
        "bower-asset/fancytree": "^2.29"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.34",
        "yiisoft/yii2-bootstrap5": "~2.0.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}

